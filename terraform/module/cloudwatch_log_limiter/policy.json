{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyCloudWatchLogging",
      "Effect": "Deny",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "*"
      ],
      "Condition": {}
    }
  ]
}